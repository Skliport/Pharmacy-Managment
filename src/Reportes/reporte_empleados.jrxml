	<queryString language="SQL">
		<![CDATA[SELECT em.id_empleado,
	em.nombre,
	em.apellido,
	em.usuario,
	ee.nombre,
	em.`fechaContratacion`,
	em.salario
FROM farmacia.empleado em
	INNER JOIN farmacia.estado_empleado ee ON 
	 em.id_estado = ee.id_estado 
ORDER BY em.id_empleado ASC]]>
	</queryString>
