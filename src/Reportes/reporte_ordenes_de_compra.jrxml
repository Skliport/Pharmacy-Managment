	<queryString language="SQL">
		<![CDATA[SELECT cm.id_compra,
	dc.id_producto,
	cm.id_empleado,
	em.nombre,
	em.apellido,
	cm.fecha,
	ps.nombre as producto,
	dc.precio_unitario,
	dc.cantidad,
	dc.precio_unitario * dc.cantidad AS total
FROM farmacia.compra cm
	join farmacia.detalle_compra dc ON 
	 cm.id_compra = dc.id_compra 
	join farmacia.producto ps ON 
	 ps.id_producto = dc.id_producto 
	join farmacia.empleado em ON 
	 cm.id_empleado = em.id_empleado 
ORDER BY cm.id_compra ASC]]>
	</queryString>
