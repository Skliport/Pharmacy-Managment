	<queryString language="SQL">
		<![CDATA[SELECT ps.id_producto,
	ps.nombre,
	ps.fabricante,
	ca.nombre AS categoria,
	pv.nombre AS proveedor,
	ps.precio_unitario,
	ps.existencia
FROM farmacia.producto ps
	join farmacia.categoria ca ON 
	 ps.id_categoria = ca.id_categoria 
	join farmacia.proveedor pv ON 
	 ps.id_proveedor = pv.id_proveedor 
ORDER BY ps.id_producto ASC]]>
	</queryString>
