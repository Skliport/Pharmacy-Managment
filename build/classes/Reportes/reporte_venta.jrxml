	<parameter name="id_orden_venta" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT vt.id_venta,
   dv.id_producto,
	vt.id_empleado,
	em.nombre,
	em.apellido,
	vt.fecha,
	ps.nombre as producto,
	dv.precio_unitario,
	dv.cantidad,
	dv.precio_unitario * dv.cantidad AS total
FROM farmacia.venta vt
	join farmacia.detalle_venta dv ON 
	 vt.id_venta = dv.id_venta 
	join farmacia.producto ps ON 
	 ps.id_producto = dv.id_producto 
	join farmacia.empleado em ON 
	 vt.id_empleado = em.id_empleado 
	 WHERE vt.id_venta =  $P{id_orden_venta} 
ORDER BY vt.id_venta ASC]]>
	</queryString>
